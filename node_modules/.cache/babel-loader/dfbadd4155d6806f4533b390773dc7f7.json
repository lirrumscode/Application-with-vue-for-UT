AdA;AAeA,IAAA,MAfA,oBAeA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,MAAA,YAAA,CAAA,KAAA,CAAA,MAAA;AACA;AAlBA;AAnBA,CAAA","sourcesContent":["<template>\n  <div>\n    <nav class=\"menu\" tabindex=\"0\">\n      <div class=\"smartphone-menu-trigger\"></div>\n      <header class=\"avatar\">\n        <img\n          src=\"../assets/logo2.png\"\n        />\n        <h2>{{ email }}</h2>\n      </header>\n      <ul>\n        <li tabindex=\"0\" class=\"icon-dashboard\">\n          <span>{{ email }}</span>\n        </li>\n        <li tabindex=\"0\" class=\"icon-settings\">\n          <span>\n            <router-link to=\"/\">\n              <a>Logout</a>\n            </router-link>\n          </span>\n        </li>\n      </ul>\n    </nav>\n    <main>\n      <div class=\"container\" id=\"contenedor\">\n        <h2>Weather App</h2>\n        <div class=\"row\">\n          <div class=\"col-sm-12 col-lg-6 col-md-6 div-container\">\n            <div class=\"card\" style=\"width: 18rem;\">\n              <h3 class=\"title\">{{ info.location.country }}</h3>\n              <span class=\"txt\">{{ info.location.name }}</span>\n              <span class=\"txt\">{{ info.location.localtime }}</span>\n              <img :src=\"info.current.weather_icons[0]\" class=\"card-img-top\" alt=\"...\" />\n              <div class=\"card-body\">\n                <h3\n                  class=\"title-description\"\n                >{{ info.current.temperature }}ยบ | {{ info.current.weather_descriptions[0] }}</h3>\n                <br>\n                <span class=\"txt-description\">Precipitation | {{ info.current.precip }}</span>\n                <span class=\"txt-description\">Feels | {{ info.current.feelslike }} ยบ</span>\n                 <span class=\"txt-description\">Wind Degree | {{ info.current.wind_degree }} ยบ</span>\n                <span class=\"txt-description\">Wind Speed | {{ info.current.wind_speed }} km/h </span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- end container -->\n    </main>\n    <Footer></Footer>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport Footer from \"@/components/Footer.vue\";\n\nexport default {\n  name: \"dashboard\",\n  components: {\n    Footer,\n  },\n\n  data() {\n    return {\n      email: localStorage.getItem(\"email\"),\n      info: null,\n      params: {\n        access_key: \"8c368817ea0c3e0f64fa10ae3ffc2f3f\",\n        query: localStorage.getItem(\"city\"),\n      },\n    };\n  },\n  mounted() {\n    this.getUser();\n  },\n  methods: {\n    async getUser() {\n      axios\n        .get(\n          \"http://api.weatherstack.com/current?access_key=\" +\n            this.params.access_key +\n            \"&query=\" +\n            this.params.query\n        )\n        .then((response) => {\n          console.log(response.data);\n          this.info = response.data;\n        })\n        .catch((e) => console.log(e));\n    },\n    logout() {\n      this.$router.push({ path: \"/\" });\n      localStorage.clear(\"city\");\n    },\n  },\n};\n</script>\n<style scoped lang=\"scss\">\n@import \"../assets/main.css\";\n@import \"../assets/card.css\";\n</style>\n"],"sourceRoot":"src/components"}]}